#include <stdio.h>

int main()
{
    int m;
    printf("Enter mark of student: ");
    scanf("%d", &m);
    
    printf("\nGrade: ");
    if(m>84 && m<=100)
    printf("A");
    else if(m>69 && m<85)
    printf("B");
    else if(m>54 && m<70)
    printf("C");
    else if(m>39 && m<55)
    printf("D");
    else if(m>=0 && m<40)
    printf("F");
    else 
    printf("Invalid Input");

    return 0;
}
